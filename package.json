{
    "name": "better-perl-syntax",
    "displayName": "Better Perl Syntax",
    "description": "An enhanced version of Perl Syntax highlighting",
    "version": "1.0.16",
    "icon": "icon.png",
    "author": {
        "name": "Jeff Hykin"
    },
    "publisher": "jeff-hykin",
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "url": "https://github.com/jeff-hykin/better-perl-syntax",
        "type": "git"
    },
    "scripts": {},
    "keywords": [
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "contributes": {
        "grammars": [
            {
                "language": "perl",
                "scopeName": "source.perl",
                "path": "syntaxes/perl.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "chalk": "^2.4.2",
        "dictionary-en-us": "^2.1.1",
        "lodash": "^4.17.19",
        "nspell": "^2.1.2",
        "ovsx": "^0.5.1",
        "textmate-bailout": "^1.1.0",
        "textmate-tester": "^1.2.0",
        "vsce": "^1.99.0",
        "yargs": "^17.5.1"
    },
    "dependencies": {
        "oniguruma": "^7.2.3"
    }
}