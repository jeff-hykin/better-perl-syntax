{
    "name": "better-nix-syntax",
    "displayName": "Better Nix Syntax",
    "description": "An enhanced version of Nix Syntax highlighting",
    "version": "2.2.3",
    "icon": "icon.png",
    "author": {
        "name": "Jeff Hykin"
    },
    "publisher": "jeff-hykin",
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "url": "https://github.com/jeff-hykin/better-nix-syntax",
        "type": "git"
    },
    "scripts": {},
    "keywords": [
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "contributes": {
        "languages": [
            {
                "id": "nix",
                "extensions": [
                    ".nix"
                ],
                "configuration": "./main/nix.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nix",
                "scopeName": "source.nix",
                "path": "./autogenerated/nix.tmLanguage.json",
                "embeddedLanguages": {
                    "source.shell": "shell"
                }
            }
        ]
    },
    "devDependencies": {
        "chalk": "^2.4.2",
        "dictionary-en-us": "^2.1.1",
        "lodash": "^4.17.19",
        "nspell": "^2.1.2",
        "ovsx": "^0.5.1",
        "textmate-bailout": "^1.1.0",
        "textmate-tester": "^1.2.0",
        "vsce": "^1.99.0",
        "yargs": "^17.5.1"
    },
    "dependencies": {
        "oniguruma": "^7.2.3"
    }
}
